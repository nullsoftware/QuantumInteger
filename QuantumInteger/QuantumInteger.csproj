<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net5.0;net6.0;net7.0;net8.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <AssemblyName>qint</AssemblyName>
    <AssemblyTitle>Quantum-resistant Integer Library</AssemblyTitle>
    <Version>1.0.12</Version>
    <AssemblyVersion>1.0.12.0</AssemblyVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <PackageId>QuantumInteger</PackageId>
    <Title>Quantum Integer</Title>
    <Description>A library for quantum-resistant integer arithmetic developed in MIT. Provides ultra-fast and secure types for quantum operations.</Description>
  </PropertyGroup>

</Project>
